test -d /root/git/xerosploit || git clone https://github.com/LionSec/xerosploit.git $(mkdir /root/git/xerosploit && echo $_)
test -d /root/git/dcrawl     || git clone https://github.com/kgretzky/dcrawl.git    $(mkdir /root/git/dcrawl && echo $_)
